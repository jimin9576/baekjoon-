#include <stdio.h>
int main(){
    int num[3], money;
    scanf("%d %d %d", &num[0], &num[1], &num[2]);
    int max = num[0];

    if((num[0]==num[1]) & (num[1]==num[2]) & (num[0]==num[2])){
        money = 10000+num[0]*1000;
        printf("%d",money);
    }
    else if((num[0]!=num[1]) & (num[1]!=num[2]) &(num[0]!=num[2])){
        for(int i=1; i<3; i++){
            if(max<num[i]){
                max=num[i];
            }
        }
        money = max*100;
        printf("%d",money);
    }
    else{
        if((num[1]==num[2] & (num[1]!=num[0]))){
            money = 1000+num[1]*100;
        }
        else if((num[0]==num[2]) & (num[0]!=num[1])){
            money = 1000+num[0]*100;
        }
        else{
            money = 1000+num[0]*100;
        }
        printf("%d",money);
    }
}
