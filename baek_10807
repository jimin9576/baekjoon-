#include <stdio.h>
int main(){
    int N,v,num[100];
    int count = 0;
    int i=0;

    scanf("%d\n", &N);
    for(int i=0; i<N; i++){
        scanf("%d", &num[i]);
    }
    scanf("%d", &v);
    for(int i=0; i<N; i++){
        if(num[i]==v){
            count++;
        }
    }
    printf("%d",count);
}
