#include <stdio.h>
int main(){
    int X, N, a[100], b[100];
    int temp=0;
    scanf("%d\n", &X);
    scanf("%d",&N);

    for(int i=0;i<N;i++){
        scanf("%d %d", &a[i], &b[i]);
        temp += a[i]*b[i];
    }
    if(temp == X){
        printf("Yes");
    }
    else{
        printf("No");
    }
}
