#include <stdio.h>
#include <string.h>
int main(){
    char S[15];
    int time_cnt=0;

    scanf("%s", &S);
    for(int i=0; i<strlen(S); i++){
        switch(S[i]){
        case 'A':
        case 'B':
        case 'C':
            time_cnt +=3;
            break;
        case 'D':
        case 'E':
        case 'F':
            time_cnt +=4;
            break;
        case 'G':
        case 'H':
        case 'I':
            time_cnt +=5;
            break;
        case 'J':
        case 'K':
        case 'L':
            time_cnt +=6;
            break;
        case 'M':
        case 'N':
        case 'O':
            time_cnt +=7;
            break;
        case 'P':
        case 'Q':
        case 'R':
        case 'S':
            time_cnt +=8;
            break;
        case 'T':
        case 'U':
        case 'V':
            time_cnt +=9;
            break;
        case 'W':
        case 'X':
        case 'Y':
        case 'Z':
            time_cnt +=10;
            break;
        default:
            time_cnt +=11;
            break;
        }
    }
    printf("%d",time_cnt);
}
