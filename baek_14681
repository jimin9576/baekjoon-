#include <stdio.h>
int main(){
    int x, y;
    scanf("%d\n", &x);
    scanf("%d", &y);

    if((-1000<=x & x<=1000 | x!=0) & (-1000<=y & y<=1000 | y!=0)){
        if(x>0 & y>0){
            printf("1");
        }
        else if(x<0 & y>0){
            printf("2");
        }
        else if(x<0 & y<0){
            printf("3");
        }
        else{
            printf("4");
        }
    }
    else{
        printf("error");
    }
}
